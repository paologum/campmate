{
  "expo": {
    "name": "campmate",
    "slug": "campmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "icon": "./assets/images/Campmate.png",
    "splash": {
      "image": "./assets/images/Campmate.png",
      "resizeMode": "contain",
      "backgroundColor": "#D4AD71"
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.campmate"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#D4AD71"
      },
      "package": "com.anonymous.campmate"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
